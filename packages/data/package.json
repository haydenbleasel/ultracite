{
  "name": "@ultracite/data",
  "description": "Shared data for Ultracite CLI and website",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "logos"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./agents": {
      "import": "./dist/agents.js",
      "types": "./dist/agents.d.ts"
    },
    "./editors": {
      "import": "./dist/editors.js",
      "types": "./dist/editors.d.ts"
    },
    "./providers": {
      "import": "./dist/providers.js",
      "types": "./dist/providers.d.ts"
    },
    "./options": {
      "import": "./dist/options.js",
      "types": "./dist/options.d.ts"
    },
    "./logos/*": "./logos/*"
  },
  "author": "Hayden Bleasel <hello@haydenbleasel.com>",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.3.4"
}
