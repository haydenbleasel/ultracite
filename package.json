    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "globals": "^15.3.0",
