  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stylelint": "^16.6.0",
    "typescript": "^5.4.5"
  },
  "name": "ultracite",
  "description": "Strict, opinionated ESLint config for modern TypeScript apps.",
  "version": "3.6.1",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haydenbleasel/ultracite.git"
  },
  "main": "./eslint.config.mjs",
  "keywords": [
    "ultracite",
    "eslint",
    "prettier",
    "stylelint",
    "typescript"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/eslint.config.mjs",
    "./prettier": "./dist/prettier.config.mjs",
    "./stylelint": "./dist/stylelint.config.mjs"
  },
  "author": "Hayden Bleasel <hello@haydenbleasel.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haydenbleasel/ultracite/issues"
  },
  "homepage": "https://github.com/haydenbleasel/ultracite#readme",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "conventional-commits",
      "first-time-contributor",
      "released"
    ]
  }
